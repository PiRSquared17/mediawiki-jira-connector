<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="com.mjcjira.jira.plugin.mjcjira-jira-rpc-plugin" name="mjcjira-jira-rpc-plugin">
  <plugin-info>
    <description>This is the Mediawiki-JIRA-Connector plugin for Atlassian JIRA.</description>
    <version>1.0</version>
    <vendor name="Mediawiki-JIRA-Connector" url="http://code.google.com/p/mediawiki-jira-connector/"/>
  </plugin-info>
  <component name="mjcjirasoapservice Component" i18n-name-key="mjcjirasoapservice.name.component" key="mjcjirasoapservice-component" class="com.mjcjira.jira.plugin.jira.rpc.mjcjirasoapserviceImpl">
    <description key="mjcjirasoapservice.description.component">Component For mjcjirasoapservice</description>
    <interface>com.mjcjira.jira.plugin.jira.rpc.mjcjirasoapservice</interface>
  </component>
  <resource type="i18n" name="i18n" location="atlassian-plugin"/>
  <rpc-soap name="mjcjirasoapservice" i18n-name-key="mjcjirasoapservice.name" key="mjcjirasoapservice" class="com.mjcjira.jira.plugin.jira.rpc.mjcjirasoapserviceImpl">
    <description key="mjcjirasoapservice.description">The mjcjirasoapservice Plugin</description>
    <service-path>mjcjirasoapservice-v1</service-path>
    <published-interface>com.mjcjira.jira.plugin.jira.rpc.mjcjirasoapservice</published-interface>
  </rpc-soap>
</atlassian-plugin>
